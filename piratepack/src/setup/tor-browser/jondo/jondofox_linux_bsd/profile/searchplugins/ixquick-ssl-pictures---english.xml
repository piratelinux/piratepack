<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Ixquick SSL Pictures - English</os:ShortName>
<os:Description>Search Ixquick, a privacy conscious metasearch engine, securely via SSL.</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/ixquick_ssl_en_pics.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/ixquick_ssl_en_pics.ico</IconUpdateUrl>
<SearchForm>https://www.ixquick.com/eng/pics.html</SearchForm>
<os:Url type="text/html" method="POST" template="https://ixquick.com/do/metasearch.pl">
  <os:Param name="query" value="{searchTerms}"/>
  <os:Param name="cat" value="pics"/>
  <os:Param name="language" value="english"/>
</os:Url>
</SearchPlugin>